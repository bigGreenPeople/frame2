<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.fj.domain.Manager" table="tb_manager">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="account" not-null="true"></property>
		<property name="name" not-null="true"></property>
		<property name="password" not-null="true"></property>
		<property name="sign" not-null="true"></property>
		
	</class>
</hibernate-mapping>